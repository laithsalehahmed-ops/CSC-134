// magic words
#include <iostream>
using namespace std;

int main() {
    // Greet the user
    cout << "Hello, user!" << endl;
    cout << "Whats your name?" << endl;
    string name;
    cin >> name;
    cout << "Nice to meet you, " << name << endl;
    
    return 0;

}